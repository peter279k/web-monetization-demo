{
    "name": "lee/web-monetization-demo",
    "description": "This is a Web Monetization Demonstration with PHP and Slim Application.",
    "support": {
        "email": "peter279k@gmail.com",
        "issues": "https://github.com/peter279k/web-monetization-demo"
    },
    "require": {
        "bryanjhv/slim-session": "~4.0",
        "monolog/monolog": "^2.1",
        "nesbot/carbon": "^2.35",
        "php-di/php-di": "^6.1",
        "slim/psr7": "^1.1",
        "slim/slim": "4.*",
        "slim/twig-view": "^3.1",
        "vlucas/phpdotenv": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
